fun Void main()
	a = 1 + 2 * 3
	assert a == 9
	b = double a + 1
	assert b == 19
	c = double: a + 1
	assert c == 20

	d = add: double 1 + 1, 2 * 2
	assert d == 7

	e = (double: a + 1) + 1
	assert e == 21

	f = add: 1, mul: 2, 3
	assert f == 7

fun Nat double(Nat x)
	x * 2

fun Nat add(Nat a, Nat b)
	a + b

fun Nat mul(Nat a, Nat b)
	a * b
